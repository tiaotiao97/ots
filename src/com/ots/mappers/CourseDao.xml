<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ots.dao.CourseDao" >


    <!-- 查询用户 -->
    <select id="selectCourseName" resultType="Course">
	    select *
	    from course
	    group by courseName;

    </select>

	<select id="selectGrade" resultType="Course">
		select *
		from course
		group by grade;

	</select>
    
    <insert id="insert" parameterType="Course">
    	insert into course(courseName,grade)
    	values(#{courseName},#{grade})
    </insert>

    
</mapper>
